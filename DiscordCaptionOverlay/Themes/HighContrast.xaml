  <SolidColorBrush x:Key="HeaderBackground" Color="#66000000"/>
  <SolidColorBrush x:Key="RowFill" Color="#33000000"/>
  <SolidColorBrush x:Key="RowBorder" Color="#77FFFFFF"/>
